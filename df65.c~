//Calculate Sum of Natural Numbers with do
#include <stdio.h>
#include <stdlib.h>
int main()
{
    int num,i,sum=0;
    printf("Enter a positive integer: ");
    scanf("%d",&num);
    i=1;
    while(i<=num)
    {
        sum+=i;   
        i++;
    }
    printf("Sum of numbers are: %d",sum);
    return 0;
}
